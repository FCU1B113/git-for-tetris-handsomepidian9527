#include <stdio.h>

typedef enum
{
    EMPTY = -1,
    I,
    J,
    L,
    O,
    S,
    T,
    Z
} Shapeid;
typedef enum
{
    RED = 41,
    GREEN,
    YELLOW,
    BLUE,
    PURPLE,
    CYAN,
    WHITE,
    BLACK = 0,

} Color;

typedef struct
{
    Shapeid shape;
    Color color;
    int size;
    char rotates[4][4][4];

} Shape;

int main()
{

    return 0;
}